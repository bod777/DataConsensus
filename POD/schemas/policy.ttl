@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix policy: <https://storage.inrupt.com/b41a41bc-203e-4b52-9b91-4278868cd036/app/schemas/policy#> .

policy:Seal a dpv:CertificationSeal, odrl:LeftOperand ;
    rdfs:label "European Data Protection Seal " .

policy:thirdPartyApproved a rdf:Property ;
    rdfs:label "Third Party Approval Status" ;
    rdfs:domain odrl:Request, odrl:Offer ;
    rdfs:range policy:Status .

policy:memberApproved a rdf:Property ;
    rdfs:label "Member Approval Status" ;
    rdfs:domain odrl:Request, odrl:Offer ;
    rdfs:range policy:Status .

policy:adminApproved a rdf:Property ;
    rdfs:label "Admin Approval Status" ;
    rdfs:domain odrl:Request, odrl:Offer ;
    rdfs:range policy:Status .

policy:Status a rdfs:Class ;
    rdfs:label "Approval Status" ;
    owl:oneOf (policy:Approved policy:Rejected policy:Pending) .

policy:Approved a policy:status ;
    rdfs:label "Approved" .
policy:Rejected a policy:status ;
    rdfs:label "Rejected" .
policy:Pending a policy:status ;
    rdfs:label "Pending" .
policy:Blocked a policy:status ;
    rdfs:label "Blocked" .