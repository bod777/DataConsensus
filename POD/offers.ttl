@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dpv: <https://w3id.org/dpv#> .
@prefix oac: <https://w3id.org/oac/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix acl: <http://www.w3.org/ns/auth/acl#> .
@prefix dpv: <https://w3id.org/dpv#> .
@prefix dpvpd: <https://w3id.org/dpv/dpv-pd#> .
@prefix ex: <http://example.com> .

ex:offer1 a odrl:Offer ;
    dct:description "To use CGM data to explore how glucose levels vary throughout the day" ;
    dct:creator ex:DataConsensus ;
    dct:issued "2022-11-08T17:26:35"^^xsd:dateTime ;
    odrl:uid ex:offer1 ;
    odrl:profile oac: ;
    odrl:permission [
        odrl:assigner ex:DataConsensus ;
        odrl:assignee ex:UniversityRequester;
        odrl:action dpv:Use, dpv:Copy, dpv:Store, dpv:Remove, dpv:Transform;
        odrl:target dpvpd:MedicalHealth;
        odrl:constraint [
            odrl:and [
                a odrl:Constraint ;
                odrl:leftOperand oac:Purpose ;
                odrl:operator odrl:isA ;
                odrl:rightOperand dpv:AcademicResearch
            ] , [
                a odrl:Constraint ;
                odrl:leftOperand oac:Purpose ;
                odrl:operator odrl:isNoneOf ;
                odrl:rightOperand dpv:CommercialResearch, dpv:SellInsightsFromData, dpv:SellDataToThirdParties
            ] , [
                a odrl:Constraint ;
                odrl:leftOperand oac:Organisation   ;
                odrl:operator odrl:isA ;
                odrl:rightOperand dpv:AcademicScientificOrganisation
            ] , [
                a odrl:Constraint ;
                odrl:leftOperand oac:TechnicalOrganisationalMeasure   ;
                odrl:operator odrl:isAllOf ;
                odrl:rightOperand dpv:CodeOfConduct, dpv:DataProtectionTraining, dpv:ConsultationWithDPO, dpv:IncidentReportingCommunication, dpv:ComplianceMonitoring, dpv:PasswordAuthentication, dpv:EncryptionInTransfer, dpv:EncryptionAtRest
            ] , [
                a odrl:Constraint ;
                odrl:leftOperand oac:Recipient  ;
                odrl:operator odrl:eq ;
                odrl:rightOperand ex:PatientGroup
            ] , [
                a odrl:Constraint ;
                odrl:leftOperand dpv:UntilTimeDuration ;
                odrl:operator odrl:eq ;
                odrl:rightOperand "2023-12-31"^^xsd:date 
            ]
        ]
    ] .


ex:offer3 a odrl:Offer ;
    dct:description """Offer to read identifier data for identity verification 
        and behavioral data for research and development""" ;
    dct:creator ex:JaneDoe ;
    dct:issued "2022-11-08T17:26:35"^^xsd:dateTime ;
    odrl:uid ex:offer3 ;
    odrl:profile oac: ;
    odrl:permission [
        odrl:assigner ex:DataConsensus ;
        odrl:assignee ex:NonprofitRequester;
        odrl:action dpv:Use, dpv:Copy, dpv:Store, dpv:Remove, dpv:Transform;
        odrl:target dpvpd:MedicalHealth;
        odrl:constraint [
            odrl:and [
                a odrl:Constraint ;
                odrl:leftOperand oac:Purpose ;
                odrl:operator odrl:isA ;
                odrl:rightOperand dpv:NonCommercialResearch
            ],
            [
                a odrl:Constraint ;
                odrl:leftOperand oac:Purpose ;
                odrl:operator odrl:isNoneOf ;
                odrl:rightOperand dpv:CommercialResearch, dpv:SellInsightsFromData, dpv:SellDataToThirdParties
            ],
            [
                a odrl:Constraint ;
                odrl:leftOperand oac:Organisation   ;
                odrl:operator odrl:isA ;
                odrl:rightOperand dpv:NonProfitOrganisation
            ],
            [
                a odrl:Constraint ;
                odrl:leftOperand oac:TechnicalOrganisationalMeasure   ;
                odrl:operator odrl:isAllOf ;
                odrl:rightOperand dpv:CodeOfConduct, dpv:DataProtectionTraining, dpv:ConsultationWithDPO, dpv:IncidentReportingCommunication, dpv:OperatingSystemSecurity, dpv:PasswordAuthentication, dpv:EncryptionInTransfer, dpv:EncryptionAtRest
            ],
            [
                a odrl:Constraint ;
                odrl:leftOperand oac:Recipient  ;
                odrl:operator odrl:eq ;
                odrl:rightOperand ex:PatientGroup
            ],
            [
                a odrl:Constraint ;
                odrl:leftOperand dpv:UntilTimeDuration ;
                odrl:operator odrl:eq ;
                odrl:rightOperand "2024-06-31"^^xsd:date
            ]
        ]
    ] .
